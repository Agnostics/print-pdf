{
	"name": "print-pdf",
	"version": "0.0.1",
	"description": "Creates batch file PDFs from SDL XPP",
	"author": "Brandon Padilla",
	"homepage": "https://github.com/agnostics/print-pdf",
	"repository": {
		"type": "git",
		"url": "https://github.com/agnostics/print-pdf.git"
	},
	"license": "MIT",
	"main": "main.js",
	"scripts": {
		"prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
		"dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode=development ",
		"build": "webpack --config webpack.build.config.js",
		"package": "webpack --config webpack.build.config.js --mode=production",
		"postpackage": "electron-builder"
	},
	"build": {
		"appId": "com.brandon.app",
		"productName": "print-pdf",
		"win": {
			"target": "portable"
		},
		"portable": {
			"requestExecutionLevel": "user"
		},
		"directories": {
			"output": "builds"
		},
		"files": [
			"package.json",
			"main.js",
			"dist/**/*"
		]
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-react": "^6.24.1",
		"babili-webpack-plugin": "^0.1.2",
		"css-loader": "^0.28.11",
		"electron": "^2.0.2",
		"electron-builder": "^20.15.1",
		"electron-packager": "^12.1.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"lru-cache": "^4.1.3",
		"node-sass": "^4.9.0",
		"react": "^16.4.0",
		"react-dom": "^16.4.0",
		"sass-loader": "^7.0.1",
		"smalltalk": "^3.0.0",
		"style-loader": "^0.21.0",
		"url-loader": "^1.0.1",
		"webpack": "^4.10.1",
		"webpack-cli": "^2.1.4",
		"webpack-dev-server": "^3.1.4"
	},
	"dependencies": {}
}
